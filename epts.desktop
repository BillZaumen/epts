[Desktop Entry]
Encoding=UTF-8
Name=EPTS
GenericName=Point and Bezier-Path Editor
Comment=Create and edit EPTS files describing points and Bezier paths
Exec=epts --gui -- %u
TryExec=epts
MimeType=application/vnd.bzdev.epts-state+xml;application/vnd.bzdev.epts-config+zip;application/vnd.bzdev.epts-template-config+zip;application/javascript;application/ecmascript;image/vnd.wap.wbmp;image/png;image/jpeg;image/bmp;image/gif;
Terminal=false
Type=Application
Icon=/usr/share/icons/hicolor/scalable/apps/epts.svg
Categories=Application;Graphics;
StartupNotify=true
Actions=stacktrace;config;tracedconfig;tconfig;tracedtconfig

[Desktop Action stacktrace]
Name = New Window with Stacktraces
Exec=epts --gui --stackTrace

[Desktop Action config]
Name = Configure Session
Exec=epts --sessionConfig

[Desktop Action tracedconfig]
Name = Configure Session with Stacktraces
Exec=epts --stackTrace --sessionConfig

[Desktop Action tconfig]
Name = Template Processing
Exec = epts --templateConfig

[Desktop Action tracedtconfig]
Name = Template Processing with Stacktraces
Exec = epts --stackTrace --templateConfig
